<Window x:Class="Practich3.Views.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practich3.Views.Windows"
        mc:Ignorable="d"
        Title="Компьютерная компания" MinHeight="450" MinWidth="800" Icon="/Resources/computer.ico" ShowInTaskbar="False">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0" Background="#FF2A96BB">
            <Button x:Name="btnBack" Content="Назад" HorizontalAlignment="Right" BorderBrush="Black" Click="btnBack_Click"/>
            <Image HorizontalAlignment="Left" Height="52" VerticalAlignment="Top" Width="51" Margin="10,4,0,0" Source="/Resources/settings.png"/>
        </Grid>
        <Frame x:Name="FrmMain" Grid.Row="1" NavigationUIVisibility="Hidden" ContentRendered="FrmMain_ContentRendered"/>
    </Grid>
</Window>
